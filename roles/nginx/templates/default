server {
	listen 80 default_server;
	server_name _;
        proxy_set_header Host $http_host;

	location / {
		proxy_pass http://localhost:8001;
	}
	
	location /nginx-metrics {
	    proxy_pass http://localhost:9113/metrics;
	}
	
	location /grafana {
	    proxy_pass http://localhost:{{ grafana_port }};
	    
	}
	
}


